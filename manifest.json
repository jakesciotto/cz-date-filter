{
  "manifest_version": 3,
  "name": "CloudZero Date Filter",
  "version": "1.0",
  "description": "Save and apply custom date filters to CloudZero Explorer",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon/cloudzero-icon-color-rgb-16.png",
      "48": "icon/cloudzero-icon-color-rgb-48.png",
      "128": "icon/cloudzero-icon-color-rgb-128.png"
    }
  },
    "web_accessible_resources": [
      {
      "resources": [
            "style/styles.css",
            "script/popup.js"
          ],
          "matches": [
            "https://app.cloudzero.com/*"
          ]
      }
    ]
  }